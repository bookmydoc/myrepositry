<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="1.6.0" client="site" method="upgrade">
  <name>AJAX Dock Cart for VirtueMart</name>
  <author>Balint Polgarfi</author>
  <authorEmail>balint.polgarfi|at|offlajn.com</authorEmail>
  <authorUrl>http://offlajn.com</authorUrl>
  <copyright>All rights reserved.</copyright>
  <version>2.37</version>
  <config>
    <fields name="params" addfieldpath="/modules/mod_ajax_dockcart/params">
      <fieldset name="basic">
        <field name="update" type="offlajnupdatechecker" label="VERSION" />
        <field name="moduleclass_sfx" type="hidden" default="-dockcart"/>
        <field name="themecache" type="list" default="0" label="THEME_CACHING" description="">
          <option value="0">DK_NO</option>
          <option value="1">YES_REFRESH_THE_CACHE</option>
          <option value="2">ALREADY_CACHED</option>
        </field>
        <field name="theme" type="theme" default="default" label="THEME" description=""/>
        <field name="module_pos" type="radio" default="0" label="POSITION">
          <option value="0">BOTTOM</option>
          <option value="1">MODULE_POSITION</option>
        </field>
        <field name="hide_empty" type="radio" default="0" label="HIDE_CART_WHEN_EMPTY" description="MOD_AJAX_DOCKCART_WORKS_ONLY_WITH_POSITION_BOTTOM">
          <option value="1">DK_YES</option>
        <option value="0">DK_NO</option>
        </field>
        <field name="notice" type="radio" default="0" label="SHOW_SUCCESS_NOTICES">
          <option value="1">DK_YES</option>
         <option value="0">DK_NO</option>
        </field>
        <field name="shadow" type="radio" default="0" label="ENABLE_SHADOW" description="DISPLAYS_SHADOW_UNDER_PRODUCTS_IN_THE_CART">
          <option value="1">DK_YES</option>
          <option value="0">DK_NO</option>
        </field>
        <field name="products" type="radio" default="1" label="SHOW_PRODUCT_ICONS">
          <option value="1">DK_YES</option>
          <option value="0">DK_NO</option>
        </field>
        <field name="ajax" type="radio" default="0" label="LOAD_PRODUCT_PAGE_WITH_AJAX" description="INSIDE_THE_WEBSHOP_WHEN_YOU_CLICK_TO_A_PRODUCT_IN_YOUR_CART_THEN_ONLY_THE_PRODUCT_AND_ITS_DETAILS_WILL_REFRESH">
          <option value="1">DK_YES</option>
          <option value="0">DK_NO</option>
        </field>
        <field type="spacer" default="LOCALISATION"/>
        <field name="cart" type="text" default="" label="SHOPPING_CART_TOOLTIP" description="LEAVE_EMPTY_IF_YOU_WANT_TO_LOAD_FROM_THE_LANGUAGE_FILES_DEFAULT_SHOPPING_CART"/>
        <field name="total" type="text" default="" label="SHOPPING_CART_LABEL" description="LEAVE_EMPTY_IF_YOU_WANT_TO_LOAD_FROM_THE_LANGUAGE_FILES_DEFAULT_TOTAL"/>
        <field name="check" type="text" default="" label="CHECKOUT_TOOLTIP" description="LEAVE_EMPTY_IF_YOU_WANT_TO_LOAD_FROM_THE_LANGUAGE_FILES_DEFAULT_CHECKOUT"/>
      </fieldset>
      <fieldset name="advanced">
        <field name="dojoload" type="radio" default="0" label="Load dojo from local server?" description="You can load the dojo framework from a local server, by default is loaded from google API.">
          <option value="1">DK_YES</option>
          <option value="0">DK_NO</option>
        </field>
      	<field name="cache" type="hidden" default="0" label="CACHING"/>
      	<field label="MODULE_TO_SHOW" name="alternative" type="text" description="MODULE_TO_SHOW_DESC" default="mod_virtuemart_cart"/>
    		<field label="EFFECT_ICONS" name="eff_icons" type="text" size="3" default="2" description="CONTROLS_HOW_MUCH_REACTION_THE_CART_MAKES_RELATIVE_TO_THE_DISTANCE_OF_THE_MOUSE_FROM_THE_CART"/>
        <field label="MIN_SIZE_OF_ICONS_PX" name="min_s" type="text" size="3" default="50" description="THE_MINIMAL_WIDTH_AND_HEIGHT_OF_THE_ICONS"/>
        <field label="NORMAL_SIZE_OF_ICONS_PX" name="nor_s" type="text" size="3" default="60" description="WIDTH_AND_HEIGHT_OF_THE_ICONS_WHEN_THE_MOUSE_IS_FAR_AWAY"/>
        <field label="MAX_SIZE_OF_ICONS_PX" name="max_s" type="text" size="3" default="110" description="WIDTH_AND_HEIGHT_OF_THE_ICONS_WHEN_THE_MOUSE_IS_DIRECTLY_OVER_IT"/>
        <field label="ICONS_MARGIN_PX" name="margin" type="text" size="3" default="5" description="MARGIN_BETWEEEN_EACH_ICON"/>
        <field label="&lt; body &gt; margin-bottom: (px)" name="marginBottom" type="text" size="3" default="0" description="MARGIN_UNDER_YOUR_PAGE"/>
        <field label="TIMEOUT_OF_AJAX_CALLS" name="timeout" type="text" size="3" default="5000" description="0 - Infinity"/>
        <field label="ADDREMOVE_PRODUCT_EFFECT_DURATION_MS" name="dur" type="text" size="3" default="300"/>
        <field label="Delaying (ms)" name="delaying" type="text" size="3" default="0" description="You can delay the loading of the dockcart."/>
        <field type="spacer" default="SPECIAL_URLS"/>
        <field name="cartUrl" type="text" default="" label="SHOW_CART_URL" description="LEAVE_EMPTY_IF_YOU_WANT_TO_USE_THE_DEFAULT"/>
        <field name="checkUrl" type="text" default="" label="CHECKOUT_URL" description="LEAVE_EMPTY_IF_YOU_WANT_TO_USE_THE_DEFAULT"/>
      </fieldset>
    </fields>
  </config>
