   background-position: top right;
}

span.nim-button-wrapper.hover>span.nim-button-l
{
   background-position: center left;
}

span.nim-button-wrapper.hover>span.nim-button-r
{
   background-position: center right;
}

span.nim-button-wrapper.active>span.nim-button-l
{
   background-position: bottom left;
}

span.nim-button-wrapper.active>span.nim-button-r
{
