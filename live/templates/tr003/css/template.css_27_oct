/* Start Box */
.nim-box, .nim-box-body {
   margin:0 auto;
   position:relative;
}
.nim-box:before, .nim-box:after, .nim-box-body:before, .nim-box-body:after {
   position:absolute;
   top:0;
   bottom:0;
   content:' ';
   background-repeat: no-repeat;
   line-height:0;
}
.nim-box:before,.nim-box-body:before{
   left:0;
}
.nim-box:after,.nim-box-body:after{
   right:0;
}
.nim-box:before {
   overflow:hidden;
   background-position: bottom left;
   direction: ltr;
   z-index:-3;
}
.nim-box:after {
   background-position: bottom right;
   z-index:-3;
}
.nim-box-body:before, .nim-box-body:after {
   background-repeat:repeat-y;
}
.nim-box-body:before {
   background-position: top left;
   z-index:-3;
}
.nim-box-body:after {
   background-position: top right;
   z-index:-3;
}

.nim-box .nim-box:before, .nim-box .nim-box:after, .nim-box-body .nim-box-body:before, .nim-box-body .nim-box-body:after {
   z-index:-2;
}
.nim-box .nim-box .nim-box:before, .nim-box .nim-box .nim-box:after, .nim-box-body .nim-box-body .nim-box-body:before, .nim-box-body .nim-box-body .nim-box-body:after {
   z-index:-1;
}
/* End Box */

/* Start Bar */
.nim-bar {
   position:relative;
}
.nim-bar:before, .nim-bar:after {
   position:absolute;
   top:0;
   bottom:0;
   content:' ';
   background-repeat:repeat;
   z-index:-1;
}
.nim-bar:before {
   left:0;
   background-position: top left;
}
.nim-bar:after {
   right:0;
   background-position: top right;
}
/* End Bar */

